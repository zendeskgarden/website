{
  "private": true,
  "name": "@zendeskgarden/website",
  "version": "0.0.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "format": "prettier-package-json --write && yarn format:all --write",
    "format:all": "prettier --loglevel warn '**/*.{js,jsx,ts,tsx,json,md,mdx,yml}' '!.github/ISSUE_TEMPLATE.md'",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:md",
    "lint:css": "stylelint 'src/**/*.{js,ts,tsx}'",
    "lint:js": "eslint ./ --ext js,ts,tsx --max-warnings 0",
    "lint:md": "markdownlint ./*.md {content,src}/**/*.{md,mdx}",
    "prepare": "git submodule update --depth 1 --init && yarn build",
    "start": "gatsby develop --open",
    "test": "prettier-package-json --list-different && yarn format:all --check && yarn lint && yarn tsc"
  },
