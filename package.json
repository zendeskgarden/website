{
  "private": true,
  "name": "@zendeskgarden/website",
  "version": "0.0.0",
  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "format": "prettier-package-json --write && yarn format:all --write",
    "format:all": "prettier --loglevel warn '**/*.{js,jsx,ts,tsx,json,md}'",
    "lint": "yarn lint:js && yarn lint:css && yarn lint:md",
    "lint:css": "stylelint 'src/**/*.{js,ts,tsx}'",
    "lint:js": "eslint ./ --ext js,ts,tsx --max-warnings 0",
    "lint:md": "markdownlint ./*.md src/**/*.md",
    "start": "gatsby develop --open",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "devDependencies": {
    "@octokit/rest": "17.0.0",
    "@types/react": "16.9.23",
    "@typescript-eslint/eslint-plugin": "2.21.0",
    "@typescript-eslint/parser": "2.21.0",
    "@zendeskgarden/eslint-config": "11.0.1",
    "@zendeskgarden/stylelint-config": "12.0.1",
    "babel-eslint": "10.1.0",
    "eslint": "6.8.0",
    "eslint-config-prettier": "6.10.0",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-notice": "0.8.9",
    "eslint-plugin-prettier": "3.1.2",
    "eslint-plugin-react": "7.18.3",
    "eslint-plugin-react-hooks": "2.5.0",
    "gatsby": "2.19.7",
    "gatsby-image": "2.2.39",
    "gatsby-plugin-manifest": "2.2.39",
    "gatsby-plugin-offline": "3.0.32",
    "gatsby-plugin-react-helmet": "3.1.21",
    "gatsby-plugin-sharp": "2.4.3",
    "gatsby-source-filesystem": "2.1.46",
    "gatsby-transformer-sharp": "2.3.13",
    "husky": "4.2.3",
    "lint-staged": "10.0.8",
    "markdownlint-cli": "0.22.0",
    "netlify": "3.1.0",
    "prettier": "1.19.1",
    "prettier-package-json": "^2.1.3",
    "prop-types": "15.7.2",
    "react": "16.12.0",
    "react-dom": "16.12.0",
    "react-helmet": "5.2.1",
    "stylelint": "13.2.0",
    "stylelint-config-styled-components": "0.1.1",
    "stylelint-order": "4.0.0",
    "stylelint-processor-styled-components": "1.10.0",
    "ts-node": "8.6.2",
    "typescript": "3.8.2"
  }
}
