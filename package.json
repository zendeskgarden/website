  "scripts": {
    "build": "gatsby build",
    "clean": "gatsby clean",
    "format": "prettier-package-json --write && npm run -- format:all --write",
    "format:all": "prettier --log-level warn '**/*.{js,jsx,ts,tsx,json,md,mdx,yml}' '!.github/ISSUE_TEMPLATE.md'",
    "postinstall": "husky install",
    "lint": "npm run lint:js && npm run lint:css && npm run lint:md",
    "lint:css": "stylelint 'src/**/*.{js,ts,tsx}'",
    "lint:js": "eslint ./ --ext js,ts,tsx --max-warnings 0",
    "lint:md": "markdownlint ./*.md {content,src}/**/*.{md,mdx}",
    "prepare": "git submodule update --depth 1 --init && npm run build",
    "start": "gatsby develop --open",
    "test": "prettier-package-json --list-different && npm run -- format:all --check && npm run lint && npm exec tsc",
    "upgrade": "utils/upgrade.js"
  },
